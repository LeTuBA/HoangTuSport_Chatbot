# Application settings
APP_NAME=Cosmetic Shop Chatbot
PORT=8000
DEBUG=false
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# API Keys
OPENAI_API_KEY=your_openai_api_key

# Spring Boot API settings
SPRING_BOOT_API_URL=http://localhost:8081
SPRING_BOOT_TOKEN=

# Vector DB settings - Qdrant
VECTOR_DB_PATH=./data/vector_db
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=products_cosmetics

# MySQL Database settings
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_NAME=cosmetic_chatbot
# Hoặc sử dụng DATABASE_URL đầy đủ
# DATABASE_URL=mysql+pymysql://username:password@localhost:3306/cosmetic_chatbot

# Model settings
EMBEDDING_MODEL=sentence-transformers/paraphrase-multilingual-MiniLM-L12-v2
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
CHAT_MODEL=gpt-3.5-turbo

# Chat settings
MAX_TOKENS=1024
TEMPERATURE=0.7

# Security
API_KEY=test-api-key 